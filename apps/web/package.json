{
  "name": "@gym-note/web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "run-p dev:vite css:types",
    "dev:vite": "vite",
    "build": "run-s css:types:build build:vite",
    "build:vite": "tsc -b && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "css:types": "tcm src --pattern '**/*.module.css' --watch",
    "css:types:build": "tcm src --pattern '**/*.module.css'"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.131.36",
    "@tanstack/react-router-devtools": "^1.131.36",
    "open-props": "^1.7.16",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@tanstack/router-plugin": "^1.131.36",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.3.3",
    "npm-run-all": "^4.1.5",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.11"
  }
}
