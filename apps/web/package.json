{
  "name": "@gym-note/web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "run-p dev:vite css:types",
    "dev:vite": "vite",
    "build": "run-s css:types:build build:vite",
    "build:vite": "tsc -b && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "css:types": "tcm src --pattern '**/*.module.css' --watch",
    "css:types:build": "tcm src --pattern '**/*.module.css'",
    "css:lint": "stylelint \"src/**/*.css\"",
    "css:fix": "stylelint \"src/**/*.css\" --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-router": "^1.131.36",
    "@tanstack/react-router-devtools": "^1.131.36",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "firebase": "^12.2.1",
    "kiso.css": "^1.2.2",
    "lucide-react": "^0.544.0",
    "open-props": "^1.7.16",
    "react": "^19.1.1",
    "react-avatar": "^5.0.4",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "sonner": "^2.0.7",
    "vaul": "^1.1.2",
    "zod": "^4.1.7",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tanstack/router-plugin": "^1.131.36",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.3.3",
    "happy-dom": "^18.0.1",
    "npm-run-all": "^4.1.5",
    "stylelint-config-recess-order": "^7.3.0",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vitest": "^3.2.4"
  }
